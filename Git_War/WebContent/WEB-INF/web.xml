<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <servlet>
    <servlet-name>MyLogin</servlet-name>
    <servlet-class>mycompany.LoginServ</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MyLogin</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Mylogout</servlet-name>
    <servlet-class>mycompany.LogoutServ</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Mylogout</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>youCheck</servlet-name>
    <servlet-class>mycompany.SessionCheck</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>youCheck</servlet-name>
    <url-pattern>/check</url-pattern>
  </servlet-mapping>
</web-app>